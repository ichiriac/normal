{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Tests",
      "request": "launch",
      "runtimeArgs": ["run-script", "test"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Blog Demo",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "DEBUG": "knex"
      },
      "program": "${workspaceFolder}/demo/blog/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch CRM Demo",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "DEBUG": "knex"
      },
      "program": "${workspaceFolder}/demo/crm/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Workers Demo",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/demo/workers/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Schema Demo",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/demo/schema/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Cache Demo (with cache)",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "CHILD": "1",
        "DEBUG": "knex:tx,knex:query"
      },
      "program": "${workspaceFolder}/demo/cache/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Stock Demo",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "DEBUG": "knex:tx,knex:query"
      },
      "program": "${workspaceFolder}/demo/stocks/index.js"
    }
  ]
}
